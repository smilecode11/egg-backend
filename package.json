{
  "name": "lego-backend2",
  "version": "1.0.0",
  "description": "乐高后端服务 2",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-lego-backend2",
    "stop": "egg-scripts stop --title=egg-server-lego-backend2",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts --resolve-plugins-relative-to .",
    "clean": "ets clean",
    "build:template:dev": "npx webpack --config webpack/webpack.config.js",
    "build:template:prod": "npx webpack --config webpack/webpack.config.js --env production && npm run upload",
    "upload": "node webpack/uploadToOSS"
  },
